package materia;

public class Teste {

	public static void main(String[] args) {

		int[] vetA;
		VetorAleatorio vetor = new VetorAleatorio();
		BubbleSort bubble = new BubbleSort();
		
		vetA = vetor.gerarVetorAleatorio(10, 20);
		
		for(int i = 0; i < vetA.length; i++){
			System.out.print(vetA[i]);
			if(i < vetA.length - 1)
				System.out.print(", ");
		}
		bubble.ordenar(vetA);
		
	}

}
