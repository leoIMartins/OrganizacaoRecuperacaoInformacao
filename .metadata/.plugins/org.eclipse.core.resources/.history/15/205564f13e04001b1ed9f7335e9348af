package materia;

public class SelectionSort {

	public void ordenar(int vet[]) {
		for(int i = 0;i < vetS.length - 1;i ++) {
			int menor = i;
			for(int j = i+1;j < vetS.length;j ++) {
				if(vetS[j] < vetS[menor]) {
					menor = j;
					trocasS ++;
				}
			}
			auxS = vetS[menor]; 
			vetS[menor] = vetS[i];
			vetS[i] = auxS;
		}
	}
}
