package materia;

public class BubbleSort {

	public void ordenar(int[] vet, int) {
		boolean troca = true;
		while(troca) {
			troca = false;
			for(int i = 0;i < vet.length - 1;i ++) {
				if(vet[i] > vet[i+1]) {
					aux = vet[i];
					vet[i] = vet[i+1];
					vet[i+1] = aux;
					troca = true;
					trocasB ++;
				}
			}
		}
	}
}
